<div class= "container user_session">
  <%= form_for(@hierarchy, :html => { :class => 'form-horizontal' }) do |f| %>

    <div class="form-group">
      <%= f.label :child_name, class: "label label-default" %><br>
      <%= f.text_field :name, class: "form-control" %>
    </div>
    <div class="form-group ">
      <%= f.label :parent_name, class: "label label-default" %><br>
      <%= f.select :parent_id, parent_options, {:include_blank => true},{class: "form-control"} %>
    </div>
    <div class="form-group actions">
      <%= f.submit 'save', class: 'btn btn-primary' %>
    </div>
  <% end %>
</div>
